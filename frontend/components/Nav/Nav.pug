nav.navbar
  div.logo uno
  div.navs-wrapper
    ul.navs
      li.navs__nav-item.nav-item
        a.navs__link(href='#') Services
      li.navs__nav-item.nav-item
        a.navs__link(href='#') About
      li.navs__nav-item.nav-item
        a.navs__link(href='#') Portfolio
      li.navs__nav-item.nav-item
        a.navs__link(href='#') Testimonials
      li.navs__nav-item.nav-item
        a.navs__link(href='#') Contact
    div.navbar-toggle-wrapper.navbar-toggle-wrapper_open
      div.navbar-toggle
        div.navbar-toggle__dash.navbar-toggle__dash_top
        div.navbar-toggle__dash.navbar-toggle__dash_middle.navbar-toggle__dash_middle_clockwise
        div.navbar-toggle__dash.navbar-toggle__dash_middle.navbar-toggle__dash_middle_conter-clockwise
        div.navbar-toggle__dash.navbar-toggle__dash_bottom
//- nav.navbar.navbar-default.navbar-static-top
  .container
    .navbar-header
      button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar
      a.navbar-brand(href='/') App Name
    #navbar.navbar-collapse.collapse
      ul.nav.navbar-nav
        li(class=(title == 'Home') ? 'active' : null)
          a(href='/') Home
        li(class=(title == 'About') ? 'active' : null)
          a(href='/about') About
      ul.nav.navbar-nav.navbar-right
        if user
          li.dropdown
            a.navbar-avatar.dropdown-toggle(href='#', data-toggle='dropdown')
              span #{user.name || user.email} &nbsp;
              i.caret
            ul.dropdown-menu
              li
                a(href='/profile') Profile
              li.divider(role='separator')
              li
                a(href='/logout') Logout
        else
          li(class=(title == 'Log in') ? 'active' : null)
            a(href='/login') Log-in
          li(class=(title == 'Register') ? 'active' : null)
            a(href='/register') Register
